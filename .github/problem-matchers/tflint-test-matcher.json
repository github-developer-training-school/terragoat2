{
  "problemMatcher": [
    {
      "owner": "tflint-test",
      "pattern": [
        {
          "regexp": "^(.+\\.tf):(\\d+):(\\d+):\\s*(error|warning|notice):\\s*(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
