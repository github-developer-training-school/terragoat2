{
  "problemMatcher": [
    {
      "owner": "tflint-test",
      "pattern": [
        {
          "regexp": "^(?:\\s*(?:Error|Warning|Info):\\s*)?(.+\\.tf):(\\d+):(\\d+):\\s*(error|warning|notice|Error|Warning|Info)?:\\s*(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
