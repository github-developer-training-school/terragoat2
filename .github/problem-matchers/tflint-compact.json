{
  "problemMatcher": [
    {
      "owner": "tflint-compact",
      "pattern": [
        {
          "regexp": "^(?:\\s*(?:Error|Warning|Info):\\s*)?(.+?):(\\d+):(\\d+):\\s*(Error|Warning|Info)?\\s*-?\\s*(.*?)(?:\\s*\\(([^)]+)\\))?$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5,
          "code": 6
        }
      ]
    }
  ]
}
